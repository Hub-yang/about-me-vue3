{
  "name": "about-me-vue3",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@10.11.0",
  "description": "about-me 个人主页 vue3版本",
  "author": "Hubery Yang",
  "license": "MIT",
  "homepage": "https://github.com/Hub-yang/about-me-vue3#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Hub-yang/about-me-vue3.git"
  },
  "bugs": {
    "url": "https://github.com/Hub-yang/about-me-vue3/issues"
  },
  "scripts": {
    "dev": "vite --open",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepare": "husky",
    "commitlint": "commitlint --edit",
    "taze": "npx taze major -r",
    "cz": "git cz"
  },
  "dependencies": {
    "@vueuse/core": "^13.3.0",
    "vite-svg-loader": "^5.1.0",
    "vue": "^3.5.15",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.13.2",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@iconify-json/carbon": "^1.2.8",
    "@unocss/eslint-plugin": "^66.1.2",
    "@unocss/reset": "^66.1.2",
    "@vitejs/plugin-vue": "^5.2.4",
    "commitizen": "^4.3.1",
    "cz-git": "^1.11.1",
    "eslint": "^9.27.0",
    "eslint-plugin-format": "^1.0.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.0.0",
    "typescript": "^5.8.3",
    "unocss": "^66.1.2",
    "unplugin-auto-import": "^19.3.0",
    "unplugin-vue-components": "^28.7.0",
    "unplugin-vue-router": "^0.12.0",
    "vite": "^6.3.5",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-vue-devtools": "^7.7.6",
    "vue-tsc": "^2.2.10"
  },
  "lint-staged": {
    "*": "eslint . --fix"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  }
