{
  "name": "about-me-vue3",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@9.10.0",
  "description": "about-me 个人主页 vue3版本",
  "author": "Hubery Yang",
  "license": "MIT",
  "homepage": "https://github.com/Hub-yang/about-me-vue3#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Hub-yang/about-me-vue3.git"
  },
  "bugs": {
    "url": "https://github.com/Hub-yang/about-me-vue3/issues"
  },
  "scripts": {
    "dev": "vite --open",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepare": "husky",
    "commitlint": "commitlint --edit",
    "taze": "npx taze major -r",
    "cz": "git cz"
  },
  "dependencies": {
    "@vueuse/core": "^10.11.1",
    "vite-svg-loader": "^5.1.0",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.27.3",
    "@commitlint/cli": "^19.6.0",
    "@commitlint/config-conventional": "^19.6.0",
    "@iconify-json/carbon": "^1.2.4",
    "@unocss/eslint-plugin": "^0.60.4",
    "@unocss/reset": "^0.60.4",
    "@vitejs/plugin-vue": "^5.2.1",
    "commitizen": "^4.3.1",
    "cz-git": "^1.11.0",
    "eslint": "npm:eslint-ts-patch@8.57.0-0",
    "eslint-plugin-format": "^0.1.2",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.10",
    "typescript": "5.6.3",
    "unocss": "^0.60.4",
    "unplugin-auto-import": "^0.17.8",
    "unplugin-vue-components": "^0.27.5",
    "unplugin-vue-router": "^0.8.8",
    "vite": "^5.4.11",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-imagemin": "^0.6.1",
    "vite-plugin-vue-devtools": "^7.6.7",
    "vue-tsc": "^2.1.10"
  },
  "lint-staged": {
    "*": "eslint . --fix"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  }
}
